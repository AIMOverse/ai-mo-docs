#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/create-vocs@1.0.0/node_modules/create-vocs/_lib/node_modules:/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/create-vocs@1.0.0/node_modules/create-vocs/node_modules:/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/create-vocs@1.0.0/node_modules:/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/create-vocs@1.0.0/node_modules/create-vocs/_lib/node_modules:/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/create-vocs@1.0.0/node_modules/create-vocs/node_modules:/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/create-vocs@1.0.0/node_modules:/Users/kelsen/Documents/Code/ai-mo-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-vocs/_lib/bin.js" "$@"
else
  exec node  "$basedir/../create-vocs/_lib/bin.js" "$@"
fi
